with "shared.gpr";
with "sdl.gpr";
project Test is
    for Source_Dirs use ("model","test");
    for Object_Dir use "../build";
    for Exec_Dir use "../bin";
    for Main use ("main.adb");

    package Compiler renames Shared.Compiler;

    package Builder is
        for Executable ("main.adb") use "test.out";
    end Builder;
end Test;
